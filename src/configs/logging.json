{
  "version": 1,
  "loggers": {
    "root": {
      "level": "DEBUG",
      "handlers": [
        "default"
      ]
    }
  },
  "handlers": {
    "default": {
      "level": "DEBUG",
      "class": "logging.StreamHandler",
      "formatter": "standard"
    }
  },
  "formatters": {
    "standard": {
      "format": "%(asctime)s [%(levelname)s] %(name)s :: %(message)s"
    }
  }
}
